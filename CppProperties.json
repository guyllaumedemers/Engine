{
  "configurations": [
    {
      "compilers": {
        "cpp": {
          "path": "${env.COMPILER_ROOT}\\clang-cl.exe"
        }
      },
      "defines": [
        "WIN32",
        "DCLSPEC_EXPORT",
        "TARGET_SDK_WINDOW_PLATFORM"
      ],
      "environments": [
        {
          "COMPILER_ROOT": "C:\\Program Files\\LLVM\\bin",
          "WIN32_ROOT": "C:\\Program Files (x86)\\Windows Kits",
          "WIN32_OS_VERSION": "10",
          "WIN32_VERSION_NUMBER": "10.0.22621.0",
          "WIN32_INCLUDES": "${env.WIN32_ROOT}\\${env.WIN32_OS_VERSION}\\Include\\${env.WIN32_VERSION_NUMBER}",
          "environment": "clang-cl"
        }
      ],
      "includePath": [
        "${workspaceRoot}\\Engine\\Source\\**",
        "${env.WIN32_INCLUDES}\\**"
      ],
      "inheritEnvironments": [
        "clang-cl"
      ],
      "intelliSenseMode": "windows-clang-x64",
      "name": "Engine-Win32-Debug"
    }
  ]
}